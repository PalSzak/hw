<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Super Awesome</title>

    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
</head>
<body class="container">

<main>
    <div class="jumbotron">
        <h1>Food Pictures</h1>
        <p>Welcome to templating using EJS</p>
    </div>

    <ul>
        <% images.forEach(function(image) { %>
            <li><%= image.name %></li>
        <% }); %>
    </ul>
</main>

</body>
</html>